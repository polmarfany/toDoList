<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.todolist.intracloud.todolist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".toDoListSimpleActivity" />
        <activity android:name=".taskActivity" />
        <activity android:name=".toDoListFilterActivity" />
        <activity
            android:name=".toDoListToolBarIconAdapter"
            android:label="@string/title_activity_to_do_list_tool_bar_icon_adapter"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>